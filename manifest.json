{
    "name": "Justin Helper Pro",
    "version": "1.1",
    "description": "Justin Helper Pro",
    "permissions": [
        "tabs",
        "cookies",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "*://*/*"
    ],
    "browser_action": {
        "default_icon": "images/lufax.png",
        "default_popup": "manager.html"
    },
    "background": {
        "scripts": ["js/jquery-1.7.1.js", "js/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*.lufax.com/*"],
            "js": ["js/jquery-1.7.1.js", "js/contentscript.js", "js/unionrsa.js"]
        }
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "web_accessible_resources":[
        "images/pre_loading.jpg"
    ]
}
